# Common across any system using bash

# Source common across bash and zsh
if [ -f "$HOME/common/csh" ]; then
    source "$HOME/common/csh"
fi

# Get ip
alias getip='ifconfig | grep 192.168'

# Source bashrc 
alias sp='source ~/.bashrc'

# Edit bash
alias ep='emacs ~/.bashrc'
alias epc='emacs ~/common/cbash'
